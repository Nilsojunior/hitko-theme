{
  "name": "Hitko Theme (Gray Editor)",
  "type": "dark",
  "colors": {
    // White
    "foreground": "#ffffff",
    "editor.foreground": "#ffffff",
    "progressBar.background": "#ffffff",
    "editorLineNumber.activeForeground": "#ffffff",
    "sideBarSectionHeader.foreground": "#ffffff",
    "sideBarTitle.foreground": "#ffffff",
    "list.inactiveSelectionForeground": "#ffffff",
    "list.hoverForeground": "#ffffff",
    "panelTitle.activeForeground": "#ffffff",
    "terminal.foreground": "#ffffff",
    "tab.activeForeground": "#ffffff",
    "tab.hoverForeground": "#ffffff",
    "list.highlightForeground": "#ffffff",
    "notifications.foreground": "#ffffff",
    "notificationCenterHeader.foreground": "#ffffff",
    "editorSuggestWidget.highlightForeground": "#ffffff",
    "editorSuggestWidget.focusHighlightForeground": "#ffffff",
    "editorHoverWidget.foreground": "#ffffff",
    "menubar.selectionForeground": "#ffffff",
    // Light Gray
    "sideBar.foreground": "#727272",
    "titleBar.activeForeground": "#727272",
    "titleBar.inactiveForeground": "#727272",
    "menu.foreground": "#727272",
    "panelTitle.inactiveForeground": "#727272",
    "tab.inactiveForeground": "#727272",
    "editorWidget.foreground": "#727272",
    "input.placeholderForeground": "#727272",
    "pickerGroup.foreground": "#727272",
    "editorSuggestWidget.foreground": "#727272",
    // "Gray"
    "editorLineNumber.foreground": "#303030",
    "editorIndentGuide.activeBackground1": "#303030",
    // Dark Gray
    "editor.background": "#0f0f0f",
    "input.background": "#0f0f0f",
    "editorWidget.background": "#0f0f0f",
    "editorSuggestWidget.background": "#0f0f0f",
    "editorHoverWidget.background": "#0f0f0f",
    "tab.activeBackground": "#0f0f0f",
    "tab.inactiveBackground": "#0f0f0f",
    "editorGroupHeader.tabsBackground": "#0f0f0f",
    // "Darker Gray"
    "sideBar.background": "#0a0a0a",
    "titleBar.activeBackground": "#0a0a0a",
    "titleBar.inactiveBackground": "#0a0a0a",
    "menu.background": "#0a0a0a",
    "panel.background": "#0a0a0a",
    "notifications.background": "#0a0a0a",
    "notificationCenterHeader.background": "#0a0a0a",
    // Transparent
    "editorOverviewRuler.border": "#00000000",
    "list.inactiveSelectionBackground": "#00000000",
    "list.activeSelectionBackground": "#00000000",
    "list.hoverBackground": "#00000000",
    "list.focusAndSelectionOutline": "#00000000",
    "menubar.selectionBackground": "#00000000",
    "panelTitle.activeBorder": "#00000000",
    "toolbar.activeBackground": "#00000000",
    "toolbar.hoverBackground": "#00000000",
    "sideBarSectionHeader.background": "#00000000",
    "tab.border": "#00000000",
    "scrollbar.shadow": "#00000000",
    "editorGroup.border": "#00000000",
    "sideBar.border": "#00000000",
    "editorIndentGuide.background1": "#00000000",
    "editorRuler.foreground": "#00000000",
    "tree.indentGuidesStroke": "#00000000",
    "titleBar.border": "#00000000",
    "menu.border": "#00000000",
    "panel.border": "#00000000",
    "terminal.border": "#00000000",
    "widget.shadow": "#00000000",
    "notificationToast.border": "#00000000",
    "notifications.border": "#00000000",
    "notificationCenter.border": "#00000000",
    "editorSuggestWidget.border": "#00000000",
    "editorHoverWidget.border": "#00000000",
    "focusBorder": "#00000000",
    // Git Colors
    "gitDecoration.addedResourceForeground": "#50d49b",
    "gitDecoration.untrackedResourceForeground": "#50d49b",
    "gitDecoration.modifiedResourceForeground": "#50d2d4",
    "gitDecoration.stageModifiedResourceForeground": "#50d2d4",
    "gitDecoration.deletedResourceForeground": "#f97583",
    "gitDecoration.stageDeletedResourceForeground": "#f97583",
    "gitDecoration.conflictingResourceForeground": "#d581e0",
    "gitDecoration.submoduleResourceForeground": "#55bbff",
    "gitDecoration.ignoredResourceForeground": "#303030"
  },
  "tokenColors": [
    {
      "name": "White",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.terminator.statement",
        "punctuation.terminator.statement",
        "punctuation.accessor",
        "punctuation.separator",
        "punctuation.definition.markdown",
        "keyword.operator.increment",
        "keyword.operator.decrement",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.type",
        "variable.object.property",
        "variable.other.property",
        "meta.embedded.expression",
        "meta.object-literal.key",
        "constant.numeric.decimal",
        "storage.type.function.arrow",
        "storage.modifier.pointer",
        "entity.name.function.operator",
        "entity.name.tag.yaml",
        "support.type.property-name",
        "text.html.markdown"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Orange",
      "scope": [
        "support.class.component",
        "variable.other.constant",
        "variable.language.this",
        "constant.language",
        "string.other.link.title"
      ],
      "settings": {
        "foreground": "#ffab70"
      }
    },
    {
      "name": "Blue",
      "scope": [
        "keyword.control.import",
        "keyword.control",
        "keyword.operator.delete",
        "keyword.operator.bitwise",
        "storage.type",
        "storage.modifier.reference",
        "meta.using-namespace",
        "entity.name.section",
        "punctuation.definition.heading",
        "new"
      ],
      "settings": {
        "foreground": "#55bbff"
      }
    },
    {
      "name": "Red",
      "scope": [
        "string.quoted.double",
        "string.quoted.other",
        "string.quoted.single",
        "entity.name.tag",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#f97583"
      }
    },
    {
      "name": "Purple",
      "scope": [
        "entity.name.function",
        "entity.name.type",
        "entity.other.attribute-name",
        "meta.export.default"
      ],
      "settings": {
        "foreground": "#d581e0"
      }
    },
    {
      "name": "Gray",
      "scope": [
        "variable.parameter",
        "variable.other.readwrite.alias",
        "variable.other.object",
        "meta.tag.without-attributes",
        "meta.body.function.definition",
        "meta.body.class",
        "support.type.primitive",
        "entity.name.namespace",
        "text.html"
      ],
      "settings": {
        "foreground": "#bdbdbd"
      }
    },
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#6d6d6d"
      }
    }
  ],
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line",
          "constant",
          "constant.character",
          "constant.character.escape",
          "constant.numeric",
          "constant.numeric.integer",
          "constant.numeric.float",
          "constant.numeric.hex",
          "constant.numeric.octal",
          "constant.other",
          "constant.regexp",
          "constant.rgb-value",
          "emphasis",
          "entity",
          "entity.name",
          "entity.name.class",
          "entity.name.function",
          "entity.name.method",
          "entity.name.section",
          "entity.name.selector",
          "entity.name.tag",
          "entity.name.type",
          "entity.other",
          "entity.other.attribute-name",
          "entity.other.inherited-class",
          "invalid",
          "invalid.deprecated",
          "invalid.illegal",
          "keyword",
          "keyword.control",
          "keyword.operator",
          "keyword.operator.new",
          "keyword.operator.assignment",
          "keyword.operator.arithmetic",
          "keyword.operator.logical",
          "keyword.other",
          "markup",
          "markup.bold",
          "markup.changed",
          "markup.deleted",
          "markup.heading",
          "markup.inline.raw",
          "markup.inserted",
          "markup.italic",
          "markup.list",
          "markup.list.numbered",
          "markup.list.unnumbered",
          "markup.other",
          "markup.quote",
          "markup.raw",
          "markup.underline",
          "markup.underline.link",
          "meta",
          "meta.block",
          "meta.cast",
          "meta.class",
          "meta.function",
          "meta.function-call",
          "meta.preprocessor",
          "meta.return-type",
          "meta.selector",
          "meta.tag",
          "meta.type.annotation",
          "meta.type",
          "punctuation.definition.string.begin",
          "punctuation.definition.string.end",
          "punctuation.separator",
          "punctuation.separator.continuation",
          "punctuation.terminator",
          "storage",
          "storage.modifier",
          "storage.type",
          "string",
          "string.interpolated",
          "string.other",
          "string.quoted",
          "string.quoted.double",
          "string.quoted.other",
          "string.quoted.single",
          "string.quoted.triple",
          "string.regexp",
          "string.unquoted",
          "strong",
          "support",
          "support.class",
          "support.constant",
          "support.function",
          "support.other",
          "support.type",
          "support.type.property-name",
          "support.variable",
          "variable",
          "variable.language",
          "variable.name",
          "variable.other",
          "variable.other.readwrite",
          "variable.parameter"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  }
}